package com.renren.intl.soundsns.rmi.test;

import org.junit.Before;
import org.junit.Ignore;
import org.junit.Test;

import com.renren.intl.soundsns.model.VersionAdmin;
import com.renren.intl.soundsns.rmi.client.SysServiceDelegate;

public class SystemTestCase {

	final SysServiceDelegate sysDelegate = new SysServiceDelegate();
	int userId ;
	String content;
	int os ;
	String version ;

	@Before
	public void setUp() throws Exception {
		 userId = 0;
		 content = "测试发送feedbacksss";
		 os = 1;
		 version = "1.04";
	}
	
    public void tearDown() throws Exception {
    	
	}
    
	@Ignore
	@Test
	public void testFeedback() {
		// 发送反馈信息
		final long start = System.currentTimeMillis();
		int success = sysDelegate.sendFeedback(GlobalValue.userId1, content, os, version);
		System.out.println("send feedback is success：" + success);
		System.out.println(System.currentTimeMillis()-start);

	}

	@Ignore
	@Test
	public void testcheckupdate() {
		// 获取版本信息
		final long start = System.currentTimeMillis();
		VersionAdmin version = sysDelegate.checkUpdate("1.0",2, "2.7");
		System.out.println("versionadmin:" + version);
		System.out.print(System.currentTimeMillis()-start);
	}
}
