package com.renren.intl.soundsns.utils;

public class Server {
	public Server(String server, String status) {
		super();
		this.name = server;
		this.status = status;
	}

	private String name;
	private String status;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

}